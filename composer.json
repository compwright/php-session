{
    "name": "compwright/php-session",
    "description": "Session implementation that does not rely on the PHP Session module or use the $_SESSION global, for use in Swoole or React-based applications",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Compwright\\PhpSession\\": "src/PhpSession/"
        },
        "files": [
            "src/PhpSession/Frameworks/Slim/registerSessionMiddleware.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Compwright\\PhpSession\\Tests\\": ["tests/features"],
            "App\\": ["tests/integration/server/App"]
        },
        "files": [
            "tests/integration/server/App/app.php"
        ]
    },
    "authors": [
        {
            "name": "Jonathon Hill",
            "email": "jonathon@compwright.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "*",
        "behat/behat": "^3.8",
        "kodus/file-cache": "^1.1",
        "matthiasmullie/scrapbook": "^1.4",
        "odan/cache": "^0.5.0",
        "league/flysystem": "^2.2",
        "bramus/monolog-colored-line-formatter": "^3.0",
        "compwright/swoole-psr7-compat": "^2.0",
        "middlewares/access-log": "^2.0",
        "monolog/monolog": "^2.3",
        "slim/psr7": "^1.4",
        "slim/slim": "^4.8",
        "php-di/php-di": "^6.3",
        "psr/log": "^1.1"
    },
    "scripts": {
        "test": [
            "php -d memory_limit=4G vendor/bin/behat"
        ],
        "lint": [
            "phpcbf -q --standard=PSR12 src tests || true",
            "phpcs --standard=PSR12 -s src tests"
        ],
        "start-php": "php -S localhost:8080 -t tests/integration/server 2> /dev/null",
        "start-swoole": "php tests/integration/server/swoole.php"
    },
    "require": {
        "psr/simple-cache": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "dflydev/fig-cookies": "^3.0"
    }
}
