{
    "name": "compwright/php-session",
    "description": "Session implementation that does not rely on the PHP Session module or use the $_SESSION global, for use in Swoole or React-based applications",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Compwright\\PhpSession\\": "src/PhpSession/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Compwright\\PhpSession\\Tests\\": ["tests/features"]
        }
    },
    "authors": [
        {
            "name": "Jonathon Hill",
            "email": "jonathon@compwright.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "*",
        "behat/behat": "^3.8",
        "kodus/file-cache": "^1.1",
        "matthiasmullie/scrapbook": "^1.4",
        "odan/cache": "^0.5.0",
        "league/flysystem": "^2.2"
    },
    "scripts": {
        "test": [
            "php -d memory_limit=4G vendor/bin/behat"
        ],
        "lint": [
            "phpcbf -q --standard=PSR12 src tests/features || true",
            "phpcs --standard=PSR12 -s src tests/features"
        ]
    },
    "require": {
        "psr/simple-cache": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "dflydev/fig-cookies": "^3.0"
    }
}
